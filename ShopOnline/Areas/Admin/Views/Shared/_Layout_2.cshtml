    <script src="/Assets_2/admin/vendor/bootstrap/js/bootstrap.min.js"></script>
    <!-- Metis Menu Plugin JavaScript -->
    <script src="/Assets_2/admin/vendor/metisMenu/metisMenu.min.js"></script>
    <!-- Morris Charts JavaScript -->
    <script src="/Assets_2/admin/vendor/raphael/raphael.min.js"></script>
    <script src="/Assets_2/admin/vendor/morrisjs/morris.min.js"></script>
    <script src="/Assets_2/admin/data/morris-data.js"></script>
    <!-- DataTables JavaScript -->
    <script src="/Assets_2/admin/vendor/datatables/js/jquery.dataTables.min.js"></script>
    <script src="/Assets_2/admin/vendor/datatables-plugins/dataTables.bootstrap.min.js"></script>
    <script src="/Assets_2/admin/vendor/datatables-responsive/dataTables.responsive.js"></script>
    <!-- Custom Theme JavaScript -->
    <script src="/Assets_2/admin/dist/js/sb-admin-2.js"></script>
    <!-- Page-Level Demo Scripts - Tables - Use for reference -->
    <script>
        $(document).ready(function () {
            $('#dataTables-example').DataTable({
                responsive: true
            });
        });
    </script>
